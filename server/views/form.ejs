                                'Otherwise you don\'t have to take any action</i></small>').show();
                    console.log($("#filePreview_" + key));
                    $f[key] = val;
                } else {
                    $("[name=" + key + "]").val(val);
                    $f[key] = val;
                }
            }
        })();
